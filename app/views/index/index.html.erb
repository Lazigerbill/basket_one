
<div class = "topbar">
	<div class = "news_window">
		<ul class = "newsticker">
		</ul>

		<script src = "https://query.yahooapis.com/v1/public/yql?q=select%20title%2C%20link%20from%20rss%20where%20url%3D%22http%3A%2F%2Ffinance.yahoo.com%2Frss%2Fheadline%3Fs%3Dtd%22&format=json&diagnostics=true&env=store%3A%2F%2Fdatatables.org%2Falltableswithkeys&callback=show_news"></script>
	</div>
</div>

<div id = "woof"></div>
<!-- This is the ticker part -->

<div class = "ticker_window">
	<div class = "ticker_window_box">	
		<div id = "company"><img class = "logo" src="http://logonoid.com/images/td-bank-logo.png" alt=""></img>Toronto Dominion Bank</div><br>
		<div id = "last">
			<img class = "tick_arrow" src="http://www.clipartbest.com/cliparts/RTG/6or/RTG6orRrc.gif" alt="">
			56.42
			<div id = "change">
				-0.45 (-0.79%)
			</div>
		</div>
		
		<div id = "pre_close">
			Previous Close: 56.87
			<div id = "last_update">Last updated: <%= Time.now %></div>
		</div>
		<div id = "range">52-week range 45.39 - 58.20</div>
	</div>
</div>


<!-- This is the Twitter window -->
<div class = "twitter_window">
	<% counter = 0 %>
	
	<ul class = "tweet_list">
		<% @tweets.each do |key, value| %>
			<li class = "tweet">
				<div class = "profile_pic"><%= image_tag (key[:profile_img_url]) %> </div>
				<div class = "tweet_content"> 
					<%= key[:text] %> <br>
					by <em><%= key[:username] %></em> <%= key[:tweet_created_at] %> 
					<span class ="retweets">retweets: <%= key[:retweet_count] %></span> <br>
				</div>
				<% counter += 1 %>
			</li>
		<% end %>
	</ul>


	There are <%= counter %> tweets!
</div>

	<%= @tweet_count %>

<!-- This is the fundamentals window -->

<div class = "fundamentals_window">
	<div class = "fundamentals_window_box">	
		<div id = "ROEChart" class = "chart"></div>
